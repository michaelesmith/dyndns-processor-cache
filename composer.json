{
    "name": "michaelesmith/dyndns-processor-cache",
    "description": "a processor for the dyndns-kit framework to allow processors to be cached",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Smith",
            "email": "sitecross@gmail.com"
        }
    ],
    "require": {
        "michaelesmith/dyndns-kit": "^0.1",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2"
    },
    "suggest": {
        "cache/cache": "Library of all the php-cache adapters",
        "symfony/cache": "Symfony Cache component with PSR-6, PSR-16, and tags"
    },
    "autoload": {
        "psr-4": {"DynDNSKit\\Processor\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"DynDNSKit\\Tests\\Unit\\": "tests/unit", "DynDNSKit\\Tests\\Common\\": "tests/common"}
    }
}
